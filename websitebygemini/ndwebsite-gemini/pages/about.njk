    <div class="absolute top-0 left-0 w-[115px] h-full bg-gradient-to-r from-nerd-light to-transparent z-10 hidden md:block"></div>
    <div class="absolute top-0 right-0 w-[115px] h-full bg-gradient-to-l from-nerd-light to-transparent z-10 hidden md:block"></div>
    <div class="flex gap-8 animate-marquee-seamless w-max hover:[animation-play-state:paused]">
      {% for expert in experts %}
      <div class="expert-card relative group w-[192px] h-[247px] md:w-[213px] md:h-[274px] flex-shrink-0 overflow-hidden cursor-pointer transition-all duration-300 rounded-[2px]">
        <img src="{{ expert.image }}" alt="{{ expert.name }}" class="w-full h-full object-cover transition-all duration-500 rounded-[2px]">
        <div class="absolute inset-0 bg-gradient-to-t from-black/80 via-black/20 to-transparent opacity-90 rounded-[2px]"></div>
        <div class="absolute bottom-0 left-0 w-full p-6 text-white">
          <p class="text-xs font-mono uppercase tracking-wider text-nerd-green mb-1">{{ expert.role }}</p>
          <h3 class="text-xl font-sans font-semibold">{{ expert.name }}</h3>
        </div>
      </div>
      {% endfor %}
      {% for expert in experts %}
      <div class="expert-card relative group w-[192px] h-[247px] md:w-[213px] md:h-[274px] flex-shrink-0 overflow-hidden cursor-pointer transition-all duration-300 rounded-[2px]">
        <img src="{{ expert.image }}" alt="{{ expert.name }}" class="w-full h-full object-cover transition-all duration-500 rounded-[2px]">
        <div class="absolute inset-0 bg-gradient-to-t from-black/80 via-black/20 to-transparent opacity-90 rounded-[2px]"></div>
        <div class="absolute bottom-0 left-0 w-full p-6 text-white">
          <p class="text-xs font-mono uppercase tracking-wider text-nerd-green mb-1">{{ expert.role }}</p>
          <h3 class="text-xl font-sans font-semibold">{{ expert.name }}</h3>
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</div>

{# Stats + Pain Points Section #}
<section class="py-16 md:py-24 bg-cream-grid relative">
