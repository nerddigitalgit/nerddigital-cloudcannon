    <div class="max-w-5xl mx-auto">
      <div class="bg-white rounded-lg border border-nerd-border p-8 md:p-12">
        {# Main Header + Stats #}
        <div class="flex flex-col md:flex-row md:items-end md:justify-between gap-6 mb-12">
          <div>
            <p class="text-xs uppercase tracking-widest text-nerd-muted mb-3 font-sans">{{ stats_eyebrow }}</p>
            <h2 class="text-3xl md:text-4xl text-nerd-dark font-sans font-semibold">{{ stats_title }} <span class="font-serif italic">{{ stats_title_italic }}</span></h2>
