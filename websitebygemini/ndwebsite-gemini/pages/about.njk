{# Simple Header for About Page #}
<div id="about-nav" class="bg-white sticky top-0 z-50 transition-all duration-300">
  <div class="container mx-auto px-6 py-4">
    <a href="/">
      <img src="{{ site.logo.header }}" alt="{{ site.title }}" class="h-5">
    </a>
  </div>
</div>
<script>
  // Nav border on scroll for about page
  document.addEventListener('DOMContentLoaded', function() {
    const aboutNav = document.getElementById('about-nav');
    if (aboutNav) {
      const updateAboutNavBorder = () => {
        if (window.scrollY > 10) {
          aboutNav.classList.add('border-b', 'border-nerd-border');
        } else {
          aboutNav.classList.remove('border-b', 'border-nerd-border');
        }
      };
      window.addEventListener('scroll', updateAboutNavBorder, { passive: true });
      updateAboutNavBorder();
    }
  });
</script>

{# Hero Section with Video #}
<section class="relative pt-16 pb-8 md:pt-20 md:pb-10 overflow-hidden">
  <div class="absolute inset-0 bg-nerd-light"></div>
  <div class="container mx-auto px-6 relative z-10">
    <div class="max-w-4xl mx-auto text-center">
      <h1 class="text-[1.71rem] md:text-[2.57rem] text-nerd-dark mb-6 leading-[1.15] font-sans font-semibold">
        {{ hero_title_start }} <span class="font-serif italic whitespace-nowrap">{{ hero_title_highlight }}</span> {{ hero_title_end }}
      </h1>
      <p class="max-w-2xl mx-auto text-base md:text-lg text-nerd-muted mb-8 leading-relaxed font-sans">
        {{ hero_subtitle }}
      </p>
      <button class="btn-primary px-8 py-4 inline-flex items-center justify-center mb-10" style="font-size: 0.6rem;" onclick="document.getElementById('calendar').scrollIntoView({behavior: 'smooth'})">
        {{ hero_cta }}
      </button>
      <div class="max-w-2xl mx-auto aspect-video rounded-lg overflow-hidden border border-nerd-border">
        <iframe src="{{ hero_video }}" class="w-full h-full" frameborder="0" allow="autoplay; fullscreen; picture-in-picture" title="About NerdDigital"></iframe>
      </div>
    </div>
  </div>
</section>

{# Expert Marquee #}
