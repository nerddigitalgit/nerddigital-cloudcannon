      <h2 class="text-3xl md:text-4xl text-nerd-dark mb-12 text-center font-sans font-bold">{{ case_studies_title }} <span class="font-serif italic">{{ case_studies_title_italic }}</span></h2>
      <div class="space-y-16">
        {% for study in case_studies %}
        <div class="bg-white rounded-lg overflow-hidden border border-nerd-border">
          <div class="p-6 pb-4">
