            <p class="text-nerd-muted font-sans text-sm leading-relaxed max-w-[140px] border-b border-nerd-border pb-4">{{ stat.description }}</p>
            <p class="text-5xl md:text-6xl text-nerd-dark font-serif">{{ stat.value }}</p>
          </div>
          {% endfor %}
        </div>

        {# Pain Points as Sub-section #}
        <div class="pt-4">
          <div class="max-w-2xl">
            <h3 class="text-xl md:text-2xl text-nerd-dark mb-8 font-sans font-semibold">{{ pain_title }} <span class="font-serif italic">{{ pain_title_italic }}</span></h3>
            <div class="space-y-4">
              {% for point in pain_points %}
              <div class="flex items-start gap-4">
                <span class="text-nerd-pink text-lg mt-0.5">—</span>
                <p class="text-base md:text-lg text-nerd-muted font-sans">{{ point }}</p>
              </div>
              {% endfor %}
            </div>
            <p class="text-lg text-nerd-dark font-sans mt-8">{{ pain_closing }} <span class="font-serif italic">{{ pain_closing_italic }}</span></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

